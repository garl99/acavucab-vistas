	<queryString>
		<![CDATA[select sum(d.cantidad_cervezas) as total_cervezas, c.nombre
from detalle_factura d, cerveza c, venta v
where v.fk_evento is not null and d.fk_venta=v.id and d.fk_cerveza=c.id and v.fecha between $P{P_STARTDATE}  and $P{P_ENDDATE}  
group by c.nombre 
order by total_cervezas desc limit 5;]]>
	</queryString>
