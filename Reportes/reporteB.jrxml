	<queryString>
		<![CDATA[select  sum(d.cantidad_cervezas) as "Total cervezas vendidas", c.nombre as "Nombre de la cerveza"
from detalle_factura d, cerveza c, venta v
where d.fk_cerveza=c.id and d.fk_venta=v.id and v.fecha between $P{P_STARTDATE} and $P{P_ENDDATE}
group by  c.nombre
order by "Total cervezas vendidas" desc limit 10;]]>
	</queryString>
